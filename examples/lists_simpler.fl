[ 0 0 2 6 6 6 7 9 ] $a$ ->
[ 3 3 4 9 14 ] $b$ ->

$a !print
$b !print

"combining..." !printstr

$i$

:loopend goto loopstart:
    ( i >= (( a @? )) ) :doit if_goto # stuff in single parens gets stuffed through shunting yard. double parens excepts from shunting yard.
    ( a @ i <= b @ 0 ) :nextiter if_goto
 
    doit: ( $a @+ i ( b @- 0 ) )
    
    nextiter: ( 1 += $i )
loopend: $b @? 0 > :loopstart if_goto

$a !print
$b !print

